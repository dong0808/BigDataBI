<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hfut.zngl.entity.Station" table="areatable">
        <id name="station_id" column="station_id">
            <generator class="assigned"/>
        </id>
        <property name="station_name" type="java.lang.String"/>
        <property name="cityName" type="java.lang.String"/>
        <property name="provName" type="java.lang.String"/>
    </class>
</hibernate-mapping>