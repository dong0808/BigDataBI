<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hfut.zngl.entity.WaterTable" table="waterTable">
        <composite-id name="waterTableID" class="com.hfut.zngl.entity.WaterTableID">
            <key-property name="timeID" column="timeID" type="java.lang.String"/>
            <key-property name="station_id" column="station_id" type="java.lang.String"/>
        </composite-id>
        <property name="rainRequ" type="float"/>
        <property name="rainFall" type="float"/>
    </class>
</hibernate-mapping>