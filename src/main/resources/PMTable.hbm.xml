<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hfut.zngl.entity.PMTable" table="pmtable">
        <composite-id name="pmTableID" class="com.hfut.zngl.entity.PMTableID">
            <key-property name="timeID" column="timeID" type="java.lang.String"/>
            <key-property name="station_id" column="station_id" type="java.lang.String"/>
        </composite-id>
        <property name="pmAve" type="java.lang.Float"></property>
    </class>
</hibernate-mapping>